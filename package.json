    "type": "git",
    "url": "git+https://github.com/rafaltrzop/listem2.git"
  },
  "bugs": {
    "url": "https://github.com/rafaltrzop/listem2/issues"
  },
  "homepage": "https://github.com/rafaltrzop/listem2#readme",
  "dependencies": {
    "concurrently": "^4.0.1"
  },
  "devDependencies": {
    "prettier": "1.15.3",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1"
  }
}
